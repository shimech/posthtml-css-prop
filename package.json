{
  "name": "@shimech/posthtml-css-prop",
  "description": "PostHTML plugin to support css prop like emotion.",
  "version": "1.0.0",
  "author": "Shuntaro Shimizu",
  "bugs": {
    "url": "https://github.com/shimech/posthtml-css-prop/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@emotion/css": "^11.9.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.30",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "posthtml": "^0.16.6",
    "posthtml-attrs-parser": "^0.1.1",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "tsup": "^5.12.6",
    "typescript": "^4.6.4"
  },
  "homepage": "https://github.com/shimech/posthtml-css-prop#readme",
  "keywords": [
    "html",
    "posthtml",
    "posthtml-plugin"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shimech/posthtml-css-prop.git"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsup ./src/index.ts --format cjs --dts --minify",
    "dev": "npm run build -- --watch",
    "lint": "eslint \"./src/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier \"./src/**/*.ts\" --write",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "prepare": "husky install"
  }
}
